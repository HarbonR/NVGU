program z_5; // 5 Обязательное

var
  a, b, c: Real; // Объявление переменных с типом Real

begin // Тело программы 
  writeln('Чтобы проверить выполняется ли неравенство (A > B > C) введите:'); // Вывод сообщения в консоль
  write('A = '); read(a); // Вывод сообщения в консоль. Запрашивает ввод данных
  write('B = '); read(b); // Вывод сообщения в консоль. Запрашивает ввод данных
  write('C = '); read(c); // Вывод сообщения в консоль. Запрашивает ввод данных
  if (a > b) and (b > c) then // Если условие соблюдается выполнится следующий код
  begin // Тело программы 
    writeln('Неравенство верно: ', a, ' > ', b, ' > ', c); // Вывод сообщения в консоль
    write('Значения удваиваются: '); // Вывод сообщения в консоль
    write('A = ', sqr(a), ' B = ', sqr(b), ' C = ', sqr(c)) // Считает и выводит результат в консоль
  end
  else // Иначе выполняется следующий код
  begin // Тело программы 
    writeln('Неравенство неверно!'); // Вывод сообщения в консоль
    write('Значения заменяются на противоположные: '); // Вывод сообщения в консоль
    a := -a; write('A = ', a); // Считает и выводит результат в консоль
    b := -b; write(' B = ', b); // Считает и выводит результат в консоль
    c := -c; write(' C = ', c) // Считает и выводит результат в консоль
  end;
end.
}
