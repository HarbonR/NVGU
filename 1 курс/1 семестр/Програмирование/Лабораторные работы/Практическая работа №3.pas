program z_2; // 2 Обязательное

var
  x, y, z: Real; // Объявление переменных с типом Real

begin // Тело программы 
  writeln('Чтобы получить max и min x, y, z введите:'); // Вывод сообщения в консоль
  write('X = '); read(x); // Вывод сообщения в консоль. Запрашивает ввод данных
  write('Y = '); read(y); // Вывод сообщения в консоль. Запрашивает ввод данных
  write('Z = '); read(z); // Вывод сообщения в консоль. Запрашивает ввод данных
  writeln('Max(X+Y+Z,X*Y*Z)'); // Вывод сообщения в консоль
  write('Max = '); // Вывод сообщения в консоль
  if x + y + z > x * y * z then // Если условие соблюдается выполнится следующий код
    writeln('X+Y+Z = ', x + y + z) // Считает и выводит результат в консоль
  else // Иначе выполняется следующий код
    writeln('X*Y*Z = ', x * y * z); // Считает и выводит результат в консоль
  writeln('Min^2(X+Y+Z/2,X*Y*Z)+1'); // Вывод сообщения в консоль
  write('Min^2 = '); // Вывод сообщения в консоль
  if sqr((x + y + z / 2) + 1) > sqr((x * y * z) + 1) then // Если условие соблюдается выполнится следующий код
    writeln('Min^2(x+y+z/2)+1 = ', sqr((x + y + z / 2) + 1)) // Считает и выводит результат в консоль
  else // Иначе выполняется следующий код
    writeln('Min^2(x*y*z)+1 = ', sqr((x * y * z) + 1)) // Считает и выводит результат в консоль
end.
}
