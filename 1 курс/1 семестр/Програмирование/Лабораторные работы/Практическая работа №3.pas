program z_1; // 1 Обязательное

var
  x, y: Real; // Объявление переменных с типом Real

begin // Тело программы 
  writeln('Чтобы получить max и min x, y введите:'); // Вывод сообщения в консоль
  write('X = '); read(x); // Вывод сообщения в консоль. Запрашивает ввод данных
  write('Y = '); read(y); // Вывод сообщения в консоль. Запрашивает ввод данных
  if x = y then // Если условие соблюдается выполнится следующий код
    writeln('X и Y равны ', x, ' = ', y) // Вывод результатa в консоль
  else if x > y then // Если условие соблюдается выполнится следующий код
    writeln('Max число X = ', x) // Вывод результатa в консоль
  else if y > x then // Если условие соблюдается выполнится следующий код
    writeln('Max число Y = ', y); // Вывод результатa в консоль
  if x < y then // Если условие соблюдается выполнится следующий код
    writeln('Min число X = ', x) // Вывод результатa в консоль
  else if y < x then // Если условие соблюдается выполнится следующий код
    writeln('Min число Y = ', y) // Вывод результатa в консоль  
end.
}
