program z_4; // 4 Обязательное

var
  a, b, c, x: Real; // Объявление переменных с типом Real

begin // Тело программы 
  writeln('Чтобы найти min значение из 3 велечин a=sin(x), b=cos(x), c=ln(x)'); // Вывод сообщения в консоль
  write('Введите X = '); read(x); // Вывод сообщения в консоль. Запрашивает ввод данных
  a := sin(x * pi / 180); writeln('a=sin(x) = ', a:0:3); // Считает и выводит результат в консоль
  b := cos(x * pi / 180); writeln('b=cos(x) = ', b:0:3); // Считает и выводит результат в консоль
  c := ln(x); writeln('c=ln(x) = ', c:0:3); // Считает и выводит результат в консоль
  if (a < b) and (a < c) then // min a
    writeln('Min a = ', a:0:3) // Вывод сообщения в консоль
  else if (b < a) and (b < c) then // min b
    writeln('Min b = ', b:0:3) // Вывод сообщения в консоль
  else if (c < a) and (c < b) then // min c
    writeln('Min c = ', c:0:3); // Вывод сообщения в консоль
  if (a > b) and (a > c) then // max a
    writeln('Max a = ', a:0:3) // Вывод сообщения в консоль
  else if (b > a) and (b > c) then // max b
    writeln('Max b = ', b:0:3) // Вывод сообщения в консоль
  else if (c > a) and (c > b) then // max c
    writeln('Max c = ', c:0:3) // Вывод сообщения в консоль
end.
}
